{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "User",
    "type": "object",
    "properties": {
      "birthday": {
        "type": "string",
        "format": "date-time",
        "description": "User's birthday as a Firestore timestamp."
      },
      "created_time": {
        "type": "string",
        "format": "date-time",
        "description": "Time when the user account was created."
      },
      "display_name": {
        "type": "string",
        "description": "The display name of the user."
      },
      "email": {
        "type": "string",
        "format": "email",
        "description": "The user's email address."
      },
      "photo_url": {
        "type": "string",
        "format": "uri",
        "description": "URL to the user's profile picture."
      },
      "uid": {
        "type": "string",
        "description": "Unique identifier for the user."
      }
    },
    "required": ["created_time", "display_name", "email", "uid"]
  }
  